model Esp3201TableRecordDiaMensual {
  id               Int     @id @default(autoincrement())
  mes              String
  avg_max_temp     Float?
  avg_min_temp     Float?
  avg_max_humidity Float?
  avg_min_humidity Float?
  moda_veleta      String?
  avg_anemometro   Float?
  sum_pluviometro  Float?
}
model Esp3201TableRecordDiaSemanal {
  id              Int      @id @default(autoincrement())
  semana          String
  start_date      DateTime
  end_date        DateTime
  max_temp        Float?
  min_temp        Float?
  max_humidity    Float?
  min_humidity    Float?
  moda_veleta     String?
  avg_anemometro  Float?
  sum_pluviometro Float?
}
model Product {
  id        Int      @id @default(autoincrement())
  name      String
  description String?
  price     Float?
  createdAt DateTime @default(now())
  image     String?
}